  },
  "scripts": {
    "test:fluxo": "npx cypress run --spec 'cypress/e2e/01-cadastrarUsuario.cy.js,cypress/e2e/02-cadastrarErroUsuario.cy.js,cypress/e2e/03-LoginUsuario.cy.js,cypress/e2e/04-LoginErroUsuario.cy.js,cypress/e2e/05-deletarConta.cy.js' --env allure=true ",
